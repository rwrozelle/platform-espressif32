{
  "build": {
    "core": "esp32",
    "extra_flags": [
      "-DARDUINO_ESP32C3_DEV",
      "-DARDUINO_USB_MODE=1",
      "-DARDUINO_USB_CDC_ON_BOOT=1"
    ],
    "f_cpu": "160000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "hwids": [
      [
        "0x303A",
        "0x1001"
      ]
    ],
    "mcu": "esp32c3",
    "variant": "esp32c3"
  },
  "connectivity": [
    "wifi",
    "bluetooth"
  ],
  "debug": {
    "openocd_target": "esp32c3.cfg"
  },
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "name": "WeAct Studio ESP32C3CoreBoard",
  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 409600,
    "maximum_size": 393216,
    "require_upload_port": true,
    "speed": 460800
  },
  "url": "https://github.com/WeActStudio/WeActStudio.ESP32C3CoreBoard",
  "vendor": "WeAct Studio"
}
